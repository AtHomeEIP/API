schema {
    query: Query
 #   mutation: Mutation
}

type Query {
    allModules: [Module]
}

#extend type Query{
#    getModuleById(id: ID!): Module
#}

type Module {
    id: ID!
    mac: String!
    name: String!
    location: String!
    type: String!
    vendor: String!
    samples: [Sample]!
}

type Sample {
    moduleId: Int
    payload: String!
    date: String!
}
